{
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "bracketSpacing": true,
    "trailingComma": "all",
    "printWidth": 80,
    "tabWidth": 4,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-tailwindcss", "@prettier/plugin-ruby"]
}
